http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="83c6e191-0a2c-4ef3-977f-08ae6dabea08" basePath="/api/v1" >
		<http:listener-connection host="0.0.0.0" port="8081" />
	</http:listener-config>
	<http:request-config name="HTTP_Request_configuration" doc:name="HTTP Request configuration" doc:id="d14d4dfe-5610-407b-9a2e-4883250384ba" >
		<http:request-connection protocol="HTTPS" host="staging.x0pa.ai" />
	</http:request-config>
	<flow name="multipartsFlow" doc:id="d68ce294-6f03-46af-abb3-94e83ae6d155" >
		<http:listener doc:name="Listener" doc:id="327af0e7-4f91-47bb-9ff9-dbcd0ffaaec8" config-ref="HTTP_Listener_config" path="/multipart"/>
		<set-payload value="" doc:name="Set Payload" doc:id="d283d0c5-f0b1-4654-a11f-c878ea5455bb" />
		<ee:transform doc:name="Transform Message" doc:id="2f44c1c2-1abd-4132-b6b0-9e40dcb4137d" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
import * from dw::core::Binaries
output application/octet-stream
---
fromBase64(payload as String) as Binary]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<ee:transform doc:name="Transform Message" doc:id="b1d0c0c1-8445-440c-8232-8ac87ccefc7c" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output multipart/form-data
boundary='34b21'
---
{
  parts : {
    file1 : {
      headers : {
        "Content-Disposition" : {
            "name": "file",
            "filename": "sample.pdf"
        },
        "Content-Type" : "application/octet-stream"
      },
      content : payload
    }
  }
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Logger" doc:id="0d78262c-d84c-4547-85f7-e9edbfd4ea4b" message="#[payload]"/>
		<http:request method="POST" doc:name="Request" doc:id="d6c6dd3b-0886-47d7-b9f5-de99ad37a0fd" config-ref="HTTP_Request_configuration" path="/roboroy/api/v1/resumes/parse">
			<http:headers ><![CDATA[#[output application/java
---
{
	"Authorization" : "tAfbDlMEDmHikhvYVXjJFf3dtBHKHo3lsPZV42m8W4GVknaAzUSZA6OLCnPMsNs4",
	"Content-Type" : "multipart/form-data; boundary=34b21"
}]]]></http:headers>
		</http:request>
		<logger level="INFO" doc:name="Logger" doc:id="57e71182-3cc5-4fd4-a2cb-da6e792fde04" message="#[payload]"/>
	</flow>
</mule>
